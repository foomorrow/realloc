  "author": "foomorrow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foomorrow/realloc/issues"
  },
  "devDependencies": {
    "babel-core": "^5.8.33",
    "babel-loader": "^5.3.3",
    "coveralls": "^2.11.4",
    "eslint": "^1.8.0",
    "istanbul": "^0.4.0",
    "jasmine": "^2.3.2",
    "webpack": "^1.12.2"
  }
}
